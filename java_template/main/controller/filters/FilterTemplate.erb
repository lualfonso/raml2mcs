package <%= package %>.controller.filters;

import <%= package %>.bean.<%= @name %>;
import <%= package %>.utils.CustomFilter;
import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

/**
 *
 * @author Luis
 */
@Data
@AllArgsConstructor
@NoArgsConstructor
public class <%= @name %>Filter extends CustomFilter<<%= @name %>>{
<% @filters.each do |filter| -%>
        private <%= filter[:type].camelize %> <%= filter[:name] %>;
<% end -%>
}
